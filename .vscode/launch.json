{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--clang-lib-dir=C:/Program Files/LLVM/bin",
                "--source=test_package/src/a/a.h"
            ]
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo run 'asc'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=asc",
                    "--package=asc"
                ],
                "filter": {
                    "name": "asc",
                    "kind": "bin"
                }
            },
            "args": [
                "--source-dir=test_package/src",
                "--entry-point-source=main.cpp",
                "--action-type=all",
                "--cmake-target-type=library",
                "--cmake-lib-type=shared"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Cargo test",
            "type": "lldb",
            "request": "launch",
            "program": "cargo",
            "args": [
                "test",
            ]
        },
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo run asc new",
            "cargo": {
                "args": [
                    "build",
                    "--bin=asc",
                    "--package=asc"
                ],
                "filter": {
                    "name": "asc",
                    "kind": "bin"
                }
            },
            "args": [
                "new",
                "--lib",
                "abc",
                "--workspace",
                "a",
                "b",
                "c"
            ],
            "cwd": "${workspaceFolder}"
        },
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo run asc scan",
            "cargo": {
                "args": [
                    "build",
                    "--bin=asc",
                    "--package=asc"
                ],
                "filter": {
                    "name": "asc",
                    "kind": "bin"
                }
            },
            "args": [
                "scan",
                "--shared-lib"
            ],
            "cwd": "${workspaceFolder}/test_package"
        },
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo run asc clean",
            "cargo": {
                "args": [
                    "build",
                    "--bin=asc",
                    "--package=asc"
                ],
                "filter": {
                    "name": "asc",
                    "kind": "bin"
                }
            },
            "args": [
                "clean"
            ],
            "cwd": "${workspaceFolder}/test_workspace"
        },
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo run asc vcpkg",
            "cargo": {
                "args": [
                    "build",
                    "--bin=asc",
                    "--package=asc"
                ],
                "filter": {
                    "name": "asc",
                    "kind": "bin"
                }
            },
            "args": [
                "vcpkg",
                "set",
                "--branch=master1"
            ],
            "cwd": "${workspaceFolder}"
        },
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo run asc vcpkg index",
            "cargo": {
                "args": [
                    "build",
                    "--bin=asc",
                    "--package=asc"
                ],
                "filter": {
                    "name": "asc",
                    "kind": "bin"
                }
            },
            "args": [
                "vcpkg",
                "index"
            ],
            "cwd": "${workspaceFolder}"
        },
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo run asc search",
            "cargo": {
                "args": [
                    "build",
                    "--bin=asc",
                    "--package=asc"
                ],
                "filter": {
                    "name": "asc",
                    "kind": "bin"
                }
            },
            "args": [
                "search",
                "spdlog",
                "--list"
            ],
            "cwd": "${workspaceFolder}"
        },
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo run asc add",
            "cargo": {
                "args": [
                    "build",
                    "--bin=asc",
                    "--package=asc"
                ],
                "filter": {
                    "name": "asc",
                    "kind": "bin"
                }
            },
            "args": [
                "add",
                "arrow",
                "--feature=json",
                "--feature=mimalloc@windows"
            ],
            "cwd": "${workspaceFolder}/test_package"
        },
    ]
}